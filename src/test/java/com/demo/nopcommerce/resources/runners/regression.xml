<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression" verbose="1" >
    <groups>
        <run>
            <include name="Regression"></include>
        </run>
    </groups>
    <test name="Login Test" >
        <classes>
            <class name="com.demo.nopcommerce.testsuit.LoginTestWithDataProvider"/>
        </classes>
    </test>
    <test name="Register Test" >
        <classes>
            <class name="com.demo.nopcommerce.testsuit.RegisterTestWithDataProvider"/>
        </classes>
    </test>
    <test name="Computer Test" >
        <classes>
            <class name="com.demo.nopcommerce.testsuit.ComputersTest"/>
        </classes>
    </test>

</suite>
